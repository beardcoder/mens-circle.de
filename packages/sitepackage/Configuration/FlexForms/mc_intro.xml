<?xml version="1.0" encoding="UTF-8"?>
<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <sheetTitle>Settings</sheetTitle>
                <type>array</type>
                <el>
                    <settings.eyebrow>
                        <label>Eyebrow Text</label>
                        <config>
                            <type>input</type>
                            <size>50</size>
                            <max>100</max>
                        </config>
                    </settings.eyebrow>
                    <settings.quote>
                        <label>Quote</label>
                        <config>
                            <type>text</type>
                            <cols>40</cols>
                            <rows>3</rows>
                        </config>
                    </settings.quote>
                    <settings.quoteAuthor>
                        <label>Quote Author</label>
                        <config>
                            <type>input</type>
                            <size>30</size>
                            <max>100</max>
                        </config>
                    </settings.quoteAuthor>
                </el>
            </ROOT>
        </sDEF>
        <sValues>
            <ROOT>
                <sheetTitle>Values</sheetTitle>
                <type>array</type>
                <el>
                    <settings.values>
                        <type>array</type>
                        <section>1</section>
                        <el>
                            <value>
                                <type>array</type>
                                <title>Value</title>
                                <el>
                                    <title>
                                        <label>Title</label>
                                        <config>
                                            <type>input</type>
                                            <size>40</size>
                                        </config>
                                    </title>
                                    <description>
                                        <label>Description</label>
                                        <config>
                                            <type>text</type>
                                            <cols>40</cols>
                                            <rows>3</rows>
                                        </config>
                                    </description>
                                </el>
                            </value>
                        </el>
                    </settings.values>
                </el>
            </ROOT>
        </sValues>
    </sheets>
</T3DataStructure>
