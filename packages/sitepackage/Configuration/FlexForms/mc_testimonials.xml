<?xml version="1.0" encoding="UTF-8"?>
<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <sheetTitle>Settings</sheetTitle>
                <type>array</type>
                <el>
                    <settings.eyebrow>
                        <label>Eyebrow Text</label>
                        <config>
                            <type>input</type>
                            <size>50</size>
                            <max>100</max>
                        </config>
                    </settings.eyebrow>
                </el>
            </ROOT>
        </sDEF>
        <sItems>
            <ROOT>
                <sheetTitle>Testimonials</sheetTitle>
                <type>array</type>
                <el>
                    <settings.items>
                        <type>array</type>
                        <section>1</section>
                        <el>
                            <item>
                                <type>array</type>
                                <title>Testimonial</title>
                                <el>
                                    <quote>
                                        <label>Quote</label>
                                        <config>
                                            <type>text</type>
                                            <cols>60</cols>
                                            <rows>4</rows>
                                        </config>
                                    </quote>
                                    <author>
                                        <label>Author Name</label>
                                        <config>
                                            <type>input</type>
                                            <size>40</size>
                                        </config>
                                    </author>
                                    <role>
                                        <label>Author Role</label>
                                        <config>
                                            <type>input</type>
                                            <size>40</size>
                                        </config>
                                    </role>
                                </el>
                            </item>
                        </el>
                    </settings.items>
                </el>
            </ROOT>
        </sItems>
    </sheets>
</T3DataStructure>
