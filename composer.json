{
  "name": "menscircle/website",
  "description": "Mens Circle Community Website - TYPO3 v14",
  "license": "GPL-2.0-or-later",
  "type": "project",
  "authors": [
    {
      "name": "Markus Sommer",
      "email": "markus@beardcoder.de",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.5",
    "menscircle/sitepackage": "dev-main",
    "typo3/cms-backend": "^14.0",
    "typo3/cms-belog": "^14.0",
    "typo3/cms-beuser": "^14.0",
    "typo3/cms-core": "^14.0",
    "typo3/cms-extbase": "^14.0",
    "typo3/cms-extensionmanager": "^14.0",
    "typo3/cms-filelist": "^14.0",
    "typo3/cms-fluid": "^14.0",
    "typo3/cms-fluid-styled-content": "^14.0",
    "typo3/cms-form": "^14.0",
    "typo3/cms-frontend": "^14.0",
    "typo3/cms-info": "^14.0",
    "typo3/cms-install": "^14.0",
    "typo3/cms-lowlevel": "^14.0",
    "typo3/cms-rte-ckeditor": "^14.0",
    "typo3/cms-scheduler": "^14.0",
    "typo3/cms-seo": "^14.0",
    "typo3/cms-setup": "^14.0",
    "typo3/cms-tstemplate": "^14.0",
    "praetorius/vite-asset-collector": "*",
    "lcobucci/jwt": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "*",
    "phpstan/phpstan": "*",
    "phpstan/extension-installer": "*"
  },
  "repositories": [
    {
      "type": "path",
      "url": "./packages/*/"
    }
  ],
  "config": {
    "allow-plugins": {
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true,
      "phpstan/extension-installer": true
    },
    "optimize-autoloader": true,
    "platform": {
      "php": "8.5.0"
    },
    "sort-packages": true
  },
  "scripts": {
    "fix": "php-cs-fixer fix",
    "lint": "php-cs-fixer fix --dry-run --diff",
    "phpstan": "phpstan analyse",
    "test": "phpunit -c packages/sitepackage/Tests/phpunit.xml"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
