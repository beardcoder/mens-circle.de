{
	"name": "mens-circle/site",
	"description": "Website https://mens-circle.de",
	"license": "GPL-2.0-or-later",
	"type": "project",
	"require": {
		"php": "^8.4",
		"ext-redis": "*",
		"ext-sodium": "*",
		"b13/container": "dev-master",
		"b13/picture": "dev-master",
		"eluceo/ical": "^2.15",
		"helhum/dotenv-connector": "^3.2.0",
		"helhum/typo3-console": "^8.2.3",
		"lcobucci/clock": "^3.5.0",
		"lcobucci/jwt": "^5.6",
		"beardcoder/queue": "@dev",
		"mens-circle/sitepackage": "@dev",
		"networkteam/sentry-client": "^5.2.1",
		"praetorius/vite-asset-collector": "^1.14",
		"spatie/icalendar-generator": "^3.1.0",
		"spatie/schema-org": "^3.23.1",
		"typo3/cms-adminpanel": ">=13.3",
		"typo3/cms-beuser": ">=13.3",
		"typo3/cms-form": ">=13.3",
		"typo3/cms-impexp": ">=13.3",
		"typo3/cms-install": ">=13.3",
		"typo3/cms-linkvalidator": ">=13.3",
		"typo3/cms-reactions": ">=13.3",
		"typo3/cms-redirects": ">=13.3",
		"typo3/cms-reports": ">=13.3",
		"typo3/cms-rte-ckeditor": ">=13.3",
		"typo3/cms-scheduler": ">=13.3",
		"typo3/cms-seo": ">=13.3",
		"typo3/cms-setup": ">=13.3",
		"typo3/cms-t3editor": ">=13.3",
		"typo3/cms-tstemplate": ">=13.3",
		"typo3/minimal": ">=13",
		"vlucas/phpdotenv": "^5.6.2"
	},
	"require-dev": {
		"b13/make": "dev-main",
		"ergebnis/composer-normalize": "^2.48.2",
		"phpro/grumphp-shim": "^2.17",
		"rector/rector": "^2.2.7",
		"roave/security-advisories": "dev-latest",
		"ssch/typo3-rector": "dev-main",
		"symfony/var-dumper": "^7.3.5",
		"symplify/easy-coding-standard": "^12.6.2",
		"typo3/cms-belog": ">=13.3",
		"typo3/cms-lowlevel": ">=13.3",
		"typo3/cms-styleguide": "^13.4.20",
		"typo3/coding-standards": "^0.8.0"
	},
	"repositories": [
		{
			"type": "path",
			"url": "./packages/*/"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"allow-plugins": {
			"ergebnis/composer-normalize": true,
			"helhum/dotenv-connector": true,
			"phpro/grumphp-shim": true,
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true
		},
		"platform": {
			"php": "8.4"
		},
		"sort-packages": true
	},
	"extra": {
		"composer-normalize": {
			"indent-size": 2,
			"indent-style": "space"
		}
	},
	"scripts": {
		"fix:php": [
			"@fix:php:cs"
		],
		"fix:php:cs": "php-cs-fixer fix --config .php-cs-fixer.php",
		"run-extension-setup": [
			"typo3 extension:setup"
		],
		"run-upgrade": [
			"typo3 language:update --skip-extension=sitepackage --skip-extension=vite_asset_collector",
			"typo3 upgrade:run"
		]
	}
}
